    "compilerOptions": {
        "target": "ESNext",
        "module": "ESNext",
        "moduleResolution": "node",
        "esModuleInterop": true,
        "resolveJsonModule": true,
        "strict": true,
        "ignoreDeprecations": "6.0",
        "forceConsistentCasingInFileNames": true
    },
    "include": [
        "src"
    ]
}